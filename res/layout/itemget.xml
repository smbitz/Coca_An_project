<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="#7F000000">
	<TextView android:text="Item Get Dialog" android:id="@+id/textView1"
		android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>

	<FrameLayout
	    android:id="@+id/frameLayout1"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content" >

		<LinearLayout
		    android:id="@+id/itemGetFertilizerA"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:orientation="vertical" android:background="@drawable/popup_congra_animals01" android:layout_gravity="center">

		</LinearLayout>

		<LinearLayout
		    android:id="@+id/itemGetFertilizerB"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:orientation="vertical" android:background="@drawable/popup_congra_animals02" android:layout_gravity="center">
		</LinearLayout>

		<LinearLayout
		    android:id="@+id/itemGetVaccineA"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:orientation="vertical" android:background="@drawable/popup_congra_plant01" android:layout_gravity="center">
		</LinearLayout>

		<LinearLayout
		    android:id="@+id/itemGetVaccineB"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:orientation="vertical" android:background="@drawable/popup_congra_plant02" android:layout_gravity="center">
		</LinearLayout>

		<LinearLayout
		    android:id="@+id/itemGetMicroorganismA"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:orientation="vertical" android:background="@drawable/popup_congra_seaanimals01" android:layout_gravity="center">

		</LinearLayout>

		<LinearLayout
		    android:id="@+id/itemGetMicroorganismB"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:orientation="vertical" android:background="@drawable/popup_congra_seaanimals02" android:layout_gravity="center">

		</LinearLayout>

		<Button
		    android:id="@+id/CloseButton"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:background="@drawable/button_close" />

	</FrameLayout>

</LinearLayout>
