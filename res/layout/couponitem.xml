<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="100dip">

    <LinearLayout
        android:id="@+id/couponItemBackground"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:orientation="vertical" android:background="@drawable/coupons_unavialble_hd">

        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" android:layout_weight="1">
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="fill_parent"
            android:layout_height="20dip"
            android:layout_weight="1" >

            <LinearLayout
                android:id="@+id/linearLayout3"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_weight="1" >
            </LinearLayout>

            <TextView
                android:id="@+id/couponItemCurrentQuantity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="x 0" android:layout_gravity="bottom"/>

            <LinearLayout
                android:id="@+id/linearLayout4"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent" android:layout_weight="1.3">
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout5"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1" >

            <LinearLayout
                android:id="@+id/linearLayout6"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent" android:layout_weight="3">
            </LinearLayout>

            <ImageView
                android:id="@+id/couponItemImage"
                android:src="@drawable/itemid5010" android:layout_width="wrap_content" android:layout_height="wrap_content"/>

            <LinearLayout
                android:id="@+id/linearLayout7"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_weight="1" >
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout12"
                android:layout_width="wrap_content"
                android:layout_height="65dip"
                android:layout_weight="3.5"
                android:orientation="vertical" android:layout_marginRight="5dip">

                <TextView
                    android:id="@+id/couponItemName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="TextView" android:layout_gravity="center"/>

                <LinearLayout
                    android:id="@+id/linearLayout14"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:orientation="vertical" >

                    <FrameLayout
                        android:id="@+id/frameLayout1"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <LinearLayout
                            android:id="@+id/linearLayout8"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent" >

                            <LinearLayout
                                android:id="@+id/linearLayout10"
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="5" >
                            </LinearLayout>

                            <TextView
                                android:id="@+id/couponItemRequireQuantity"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="4"
                                android:text="TextView" android:layout_gravity="center"/>

                            <LinearLayout
                                android:id="@+id/linearLayout9"
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="1" >
                            </LinearLayout>
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/couponItemStatusImage"
                            android:layout_width="wrap_content"
                            android:layout_height="62dp"
                            android:layout_gravity="right"
                            android:src="@drawable/text_unavailable_hd" />

                    </FrameLayout>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>
    
</LinearLayout>
